using System;
using System.Collections.Generic;
using System.Linq;

namespace Arrays
{
    class Program
    {
        static void Main(string[] args)
        {
            //ex.->  Change without other variable
            
            int x = 10;
            int y = 5;

            x = x + y;
            y = x - y;
            x = x - y;
            Console.WriteLine("After swapping: x = " + x + ", y = " + y);
        }
    }
}
